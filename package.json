{
  "name": "audio-organizer",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "organize": "./audio-organizer",
    "start": "node dist/index.js",
    "build": "tsc",
    "compile": "bun build index.ts --compile --outfile audio-organizer",
    "watch": "tsc --watch",
    "dev": "node --watch dist/index.js",
    "lint": "eslint . --ext .ts",
    "tsc": "tsc"
  },
  "author": "",
  "license": "ISC",
  "description": "Organizes all iPod Classic supported audio files (MP3, M4A, AAC, AIFF, WAV, etc.) by artist and album based on metadata",
  "devDependencies": {
    "@types/node": "^22.15.21",
    "bun": "^1.2.14",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@types/inquirer": "^9.0.8",
    "iconv-lite": "^0.6.3",
    "inquirer": "^12.6.3",
    "music-metadata": "^11.2.3"
  },
  "type": "module"
}
